<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>Demo JSP</display-name>
	
	<welcome-file-list>
		<welcome-file>identification.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<description>Validation du formulaire de login</description>
		<display-name>ValiderForm</display-name>
		<servlet-name>ValiderForm</servlet-name>
		<servlet-class>com.formation.jstlel.ValiderForm</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ValiderForm</servlet-name>
		<url-pattern>/ValiderForm</url-pattern>
	</servlet-mapping>

</web-app>